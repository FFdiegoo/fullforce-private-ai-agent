{
  "name": "fullforce-private-ai-agent",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "node scripts/security-scan.js",
    "security:report": "node scripts/security-report.js",
    "security:test": "node scripts/test-security.js",
    "security:full-audit": "node scripts/dependency-audit.js",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "node scripts/dependency-audit.js",
    "schema:verify": "node scripts/verify-schema-compliance.js",
    "schema:check": "npm run schema:verify",
    "schema:sync": "node scripts/prisma-sync-check.js",
    "prisma:sync": "npm run schema:sync",
    "monitor:errors": "node -e \"console.log('Error monitoring is active in the application')\"",
    "audit:all": "npm run security:full-audit && npm run schema:sync && npm run security:test"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-table": "^8.11.8",
    "autoprefixer": "^10.4.17",
    "date-fns": "^3.3.1",
    "dotenv": "^16.6.1",
    "formidable": "^2.1.1",
    "helmet": "^7.2.0",
    "next": "14.1.0",
    "openai": "^4.0.0",
    "postcss": "^8.4.35",
    "qrcode": "^1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.11.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-security": "^1.7.1",
    "prisma": "^6.9.0",
    "typescript": "^5.8.3"
  }
}